{
    "dataset": {
        "file": "datasets/TSP/1.txt",
        "epochs":30000,
        "neighbourhood": {
            "initial": 5,
            "decay": 5000,
            "function": "exponential_decay"
        },
        "learning rate": {
            "initial": 0.6,
            "decay": 7000,
            "function": ""
        }
    },
    "random range": [0, 1.0],
    "node multiplier": 2,
    "normalize": {
        "use": true,
        "feature independant": false
    },
    "visuals": {
        "on": true,
        "title": "Travelling Salesman Problem",
        "refresh rate": 1,
        "print rate": 1
    }
}