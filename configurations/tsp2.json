{
    "dataset": {
        "file": "datasets/TSP/2.txt",
        "epochs":15000,
        "neighbourhood": {
            "initial": 4,
            "decay": 3500,
            "function": "exponential_decay"
        },
        "learning rate": {
            "initial": 0.6,
            "decay": 1000,
