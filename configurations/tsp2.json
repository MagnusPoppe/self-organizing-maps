    "random range": [0,1],
    "node multiplier": 2,
    "normalize": {
        "use": true,
        "feature independant": true
    },
    "visuals": {
        "on": true,
        "title": "Travelling Salesman Problem",
        "refresh rate": 1,
        "print rate": 1
    }
}